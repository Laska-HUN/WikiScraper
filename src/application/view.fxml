<AnchorPane prefHeight="420.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Pane prefHeight="420.0" prefWidth="650.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="url" layoutX="20.0" layoutY="15.0" prefHeight="30.0" prefWidth="300.0" promptText="Wikipedia page url" />
            <Button fx:id="get" layoutX="500.0" layoutY="15.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Get Table Content">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <TextArea fx:id="result" editable="false" layoutX="20.0" layoutY="55.0" prefHeight="316.0" prefWidth="610.0" promptText="Scrape results" />
            <Button fx:id="upload" layoutX="260.0" layoutY="380.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Upload to Database">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <ProgressIndicator fx:id="wait" layoutX="275.0" layoutY="160.0" prefHeight="100.0" prefWidth="100.0" visible="false" />
            <ChoiceBox fx:id="choice" layoutX="335.0" layoutY="15.0" prefHeight="30.0" prefWidth="150.0" />
         </children>
      </Pane>
   </children>
